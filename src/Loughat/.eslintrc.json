{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "eslint:recommended",
    "no-undef": 1,
    "no-unused-vars": 1,
    "object-curly-spacing": 1,
    "globals": {
        "$": true
    }
}